{
    "name": "jwtauth",
    "version": "1.0.0",
    "description": "A Sample node.js app to depict jwt implemenation and other best prctices",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node index.js",
        "build": "lwc-services build --mode production",
        "build:development": "lwc-services build",
        "lint": "eslint ./src/**/*.js",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,yaml,yml}\"",
        "serve": "node scripts/server.js",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "lwc-services watch",
        "watch:production": "lwc-services watch --mode=production"
    },
    "author": "Aditya Krishna",
    "license": "ISC && CC0-1.0 ",
    "dependencies": {
        "@hapi/joi": "17.1.1",
        "@uirouter/angularjs": "1.0.26",
        "angular": "1.7.9",
        "aws-sdk": "2.734.0",
        "bcrypt": "^5.0.1",
        "bcryptjs": "2.4.3",
        "body-parser": "1.19.0",
        "bootstrap": "4.5.0",
        "compression": "^1.7.4",
        "crypto-js": "4.0.0",
        "dotenv": "8.2.0",
        "epxress": "0.0.1-security",
        "express": "4.17.1",
        "fs": "0.0.1-security",
        "helmet": "^4.4.1",
        "jquery": "3.5.1",
        "jsonwebtoken": "8.5.1",
        "nodemon": "2.0.4",
        "pg": "8.2.1",
        "sequelize": "4.44.4",
        "sequelize-values": "1.1.0",
        "yarn": "^1.22.10"
    },
    "devDependencies": {
        "@sa11y/jest": "0.2.1",
        "@salesforce/wire-service-jest-util": "^2.4.2",
        "chart.js": "^2.9.4",
        "d3": "^6.5.0",
        "husky": "^4.3.8",
        "jest-canvas-mock": "^2.3.0",
        "lint-staged": "^10.5.2",
        "lwc-recipes-oss-ui-components": "^0.3.2",
        "lwc-services": "^1.2.2",
        "moment": "^2.29.1",
        "prettier": "^2.2.1"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "lwc"
    ],
    "lint-staged": {
        "**/*.{css,html,js,json,yaml,yml,md}": [
            "prettier --write"
        ],
        "**/modules/**/*.js": [
            "eslint"
        ]
    },
    "repository": "trailheadapps/lwc-recipes-oss"
}
